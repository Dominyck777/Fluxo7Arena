# JSONBin Configuration for ISIS Feedback
VITE_JSONBIN_BIN_ID=your_jsonbin_bin_id
VITE_JSONBIN_API_KEY=your_jsonbin_api_key

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Maintenance Mode (optional)
# When true, the app shows the premium Maintenance page and blocks all routes except /maintenance
# Example end datetime uses ISO8601 with timezone
VITE_MAINTENANCE_MODE=true
VITE_MAINTENANCE_END="2025-12-04T23:00:00-03:00"
# Optional custom text to override default message
VITE_MAINTENANCE_TEXT=
